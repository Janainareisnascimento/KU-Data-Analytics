    {
      "cell_type": "code",
      "source": [
        "print(\"docs1: {}\".format(docs1))\n",
        "print(\"mystopwords: {}\".format(mystopwords))\n",
        "print(\"dtm1: {}\".format(dtm1))\n",
        "print(\"tf_dict1: {}\".format(tf_dict1))"
      ],
      "metadata": {
